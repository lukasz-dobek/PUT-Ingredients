{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":preserveSemverRanges",
  ],
  "labels": ["dependencies"],
  "reviewers": ["@lukasz-dobek"],
  "prHourlyLimit": 5,
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "addLabels": ["javascript"],
      "additionalReviewers": [],
    },
    {
      // The following rule
      "matchPackageNames": ["express-handlebars", "express"],
      "groupName": "express deps",
    }
  ],
}
